{
  "name": "HI6MA",
  "version": "1.3.0",
  "description": " A tiny comic browser and reader based in Vuejs.",
  "main": "package.json",
  "repository": "https://github.com/bersLucas/HI6MA.git",
  "author": "Lucas Bersier <contact@lucasbersier.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "inline-source-cli": "^1.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.15.2"
  },
  "scripts": {
    "pug": "pug src/index.pug -o dist",
    "build": "yarn run pug && yarn run sass && yarn run inline",
    "inline": "inline-source --compress dist/index.html dist/index.html",
    "sass": "sass src/style/style.scss src/style/style.css --style compressed"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "vue": "^2.5.21"
  }
}
